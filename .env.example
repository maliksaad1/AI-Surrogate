# API Keys (Replace these with your actual API keys)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
WHATSAPP_ACCOUNT_SID=your_twilio_account_sid_here
WHATSAPP_AUTH_TOKEN=your_twilio_auth_token_here
WHATSAPP_PHONE_NUMBER=+1234567890

# Google Cloud APIs
# Download your Google Cloud credentials JSON file and specify the path
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/google-credentials.json
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here
GOOGLE_DOCS_API_KEY=your_google_docs_api_key_here
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here

# Text-to-Speech (Choose one provider and fill in the corresponding keys)
TTS_PROVIDER=azure  # Options: azure, amazon_polly, elevenlabs
AZURE_TTS_KEY=your_azure_tts_key_here
AZURE_TTS_REGION=your_azure_region_here
AMAZON_POLLY_KEY=your_amazon_polly_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Database (Update with your database credentials)
DATABASE_URL=postgresql://user:password@localhost:5432/ai_surrogate

# Security (Generate strong random keys)
JWT_SECRET_KEY=generate_a_strong_random_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Application Settings
DEBUG=True
ENVIRONMENT=development
LOG_LEVEL=INFO 